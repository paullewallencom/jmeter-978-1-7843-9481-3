            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.*

// Turn the JSON into an object called &apos;response&apos;
def response = prev.getResponseDataAsString
def json = new JsonSlurper().parseText(response)

vars.put(&quot;url_cnt&quot;, json.responseData.results.length);

for (int i = 0; i &lt;= response.responseData.results.length; i++)	
{		
    def result = json.response.responseData.results[i];
   	vars.put(&quot;url_&quot; + i, result.url);
}</stringProp>
